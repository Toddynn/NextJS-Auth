{
  "version": "0.2.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "vercel-build": "npx typedoc src/index.ts"
  },
  "peerDependencies": {
    "next": ">=9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "next-swagger-doc",
  "keywords": [
    "next-swagger",
    "next-swagger-doc",
    "nextjs",
    "swagger"
  ],
  "author": "Huynh Duc Dung",
  "repository": {
    "type": "git",
    "url": "https://github.com/jellydn/next-swagger-doc"
  },
  "module": "dist/next-swagger-doc.esm.js",
  "exports": {
    "require": "./dist/index.js",
    "import": "./dist/next-swagger-doc.esm.js"
  },
  "size-limit": [
    {
      "path": "dist/next-swagger-doc.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/next-swagger-doc.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "7.0.8",
    "@skypack/package-check": "0.2.2",
    "@types/swagger-jsdoc": "6.0.1",
    "all-contributors-cli": "6.20.0",
    "husky": "7.0.4",
    "next": "12.0.10",
    "size-limit": "7.0.8",
    "tsdx": "0.14.1",
    "tslib": "2.3.1",
    "typedoc": "0.22.11",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "isarray": "^2.0.5",
    "swagger-jsdoc": "^6.1.0"
  }
}
